# Example/default config script for boxbuilder
export boxbuilder_chef_repos="git://github.com/thewoolleyman/boxbuilder_example_chef_config.git git://github.com/thewoolleyman/boxbuilder_example1_chef_repo.git git://github.com/thewoolleyman/boxbuilder_example2_chef_repo.git"
export boxbuilder_chef_config_path="$HOME/.chef/boxbuilder_example_chef_config/config/solo.rb"
export boxbuilder_chef_json_path="$HOME/.chef/boxbuilder_example_chef_config/config/node.json"

# Default values for building AMIs
export boxbuilder_ami_instancetype=m1.large
export boxbuilder_ami_prefix="built-by-boxbuilder"

