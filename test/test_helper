trap 'echo BOXBUILDER - boxbuilder_test: error on line $LINENO' ERR

assert_equal() {
  set +o nounset
  if [[ ! $1 = $2 ]]; then
    echo "Expected $2 to be equal to $1"
    echo `caller 0`
    set -o nounset
    return 1
  fi
  set -o nounset
}
